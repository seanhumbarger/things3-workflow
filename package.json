{
  "name": "things3-workflow",
  "version": "0.1.0",
  "description": "Import tasks from the Things3 SQLite database into Obsidian notes. Supports filtering by tags, projects, and areas, custom note headers, custom tags, project/area as tags, checklist and body import, date metadata, and a local cache to prevent duplicate imports. Uses sql.js for compatibility and is desktop-only.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs --watch",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint . --ext .ts",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "things3",
    "importer",
    "tasks",
    "sqlite",
    "native"
  ],
  "author": "Sean Humbarger",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.19.11",
    "esbuild-plugin-obsidian": "^0.1.0",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "obsidian": {
    "name": "Things3 Workflow",
    "isDesktopOnly": true,
    "minAppVersion": "0.15.0"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "sql.js": "^1.13.0",
    "uuid": "^13.0.0"
  }
}
